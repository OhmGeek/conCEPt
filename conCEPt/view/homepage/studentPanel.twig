{% for student in students %}
<div class = "panel">
	<div class = "panel-heading">
		<h4 class = "panel-title">
			<a class = ""
href="#{{student.studentName}}">{{student.studentName}}</a>
		</h4>
	<div>
	<div id="{{student.studentName}}" class = "panel">
		<div class = "panel-body">
			<table class ="table text-center">
				<thead>
					<tr>
						<th class="text-center">Document</th>
						<th class="text-center">Submitted</th>
						<th class="text-center">Merged</th>
					</tr>
				</thead>
				<tbody>
					{% for doc in student.forms %}
					<tr class = "{{doc.type}}"> 
						<td>{{doc.title}}</td>
							<td><a
href="{{doc.submitted_link}}">{% if doc.submitted %} Submitted {% else %} Not Submitted {% endif %}</a></td>
							<td><a
href="{{doc.linkMerged}}">{% if doc.merged %} Merged{% endif %}</a></td>
					</tr>
					{% endfor %}
				</tbody>
			</table>
		</div>
	</div>
</div>
{% endfor %}
