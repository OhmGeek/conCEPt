{% for student in students %}
<div class = "panel panel-default">
	<div class = "panel-heading">
		<h4 class = "panel-title">
			<a class = "btn-block" data-toggle="collapse"
href="#{{student.studentName}}">{{student.studentName}}</a>
		</h4>
	</div>
	<div id="{{student.studentName}}" class = "panel-collapse collapse">
		<div class = "panel-body">
			<table class ="table text-center">
				<thead>
					<tr>
						<th class="text-center">Document</th>
						<th class="text-center">Submitted</th>
						<th class="text-center">Merged</th>
					</tr>
				</thead>
				<tbody>
					{% for doc in student.forms %}
					<tr class = {{doc.type}}> 
						<td>{{doc.title}}</td>
							<td> {{doc.submitted}} {{doc.submitted_link}}</td>
							<td> {{doc.merged}} {{doc.linkMerged}}</td>
					</tr>
					{% endfor %}
				</tbody>
			</table>
		</div>
	</div>
</div>
{% endfor %}
