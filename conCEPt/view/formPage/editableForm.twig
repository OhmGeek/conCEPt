<!--EDITABLE FORM FOR EXAMINER AND SUPERVISOR REPORTS-->

<h1>{{title}}</h1> 
<h2>Individual {{markerType}}'s Report</h2>
<div class = "header-details">
	<div class = "header-input">
		<h4>Student Name: {{studentName}}</h4>
	</div> 
	<div class="header-input">
		<h4>{{markerType}} Name: {{markerName}}</h4>
	</div>
</div>

{{table|raw}}

<div class = "general-comments">
	General comments on the paper<br>
	<div class = "comments"><textarea form="documentID" name="comments"> {{comments}} </textarea></div>
</div>

<!-- Change these to buttons instead? -->
<form class = "table" method="" action="#" id ="documentID">
	<input type="submit" name = "submit" value="Submit" disabled/>
	<input type="submit" name = "save" value="Save"/>
</form>